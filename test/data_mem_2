#! /usr/remote/bin/vvp
:ivl_version "10.1 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x18afdb0 .scope module, "test" "test" 2 3;
 .timescale 0 0;
P_0x18aff30 .param/l "END_COUNT" 0 2 13, +C4<00000000000000000001000000000011>;
P_0x18aff70 .param/l "LOWER_ADDR" 0 2 14, C4<01111111111111110000000000000000>;
P_0x18affb0 .param/l "UPPER_ADDR" 0 2 15, C4<01111111111111111111111111111100>;
v0x18c75c0_0 .var "addr", 31 0;
v0x18c76a0_0 .var "clk", 0 0;
v0x18c7740_0 .var "count", 1 0;
v0x18c7830_0 .var "mem_write", 0 0;
v0x18c7900_0 .net "read_data", 31 0, v0x18c7330_0;  1 drivers
v0x18c79f0_0 .var "write_data", 31 0;
E_0x18abb70 .event edge, v0x18c7740_0;
S_0x18ab670 .scope module, "data_mem" "data_memory" 2 19, 3 13 0, S_0x18afdb0;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "mem_write"
    .port_info 1 /INPUT 32 "address"
    .port_info 2 /INPUT 32 "write_data"
    .port_info 3 /OUTPUT 32 "read_data"
v0x18ab870_0 .net "address", 31 0, v0x18c75c0_0;  1 drivers
v0x18c70c0 .array "data_mem", 2147483644 2147418112, 31 0;
v0x18c7180_0 .var/i "i", 31 0;
v0x18c7270_0 .net "mem_write", 0 0, v0x18c7830_0;  1 drivers
v0x18c7330_0 .var "read_data", 31 0;
v0x18c7460_0 .net "write_data", 31 0, v0x18c79f0_0;  1 drivers
E_0x18ac100 .event edge, v0x18c7330_0, v0x18ab870_0, v0x18c7270_0;
    .scope S_0x18ab670;
T_0 ;
    %pushi/vec4 0, 0, 32;
    %assign/vec4 v0x18c7330_0, 0;
    %pushi/vec4 2147418112, 0, 32;
    %store/vec4 v0x18c7180_0, 0, 32;
T_0.0 ;
    %load/vec4 v0x18c7180_0;
    %cmpi/u 2147483644, 0, 32;
    %jmp/0xz T_0.1, 5;
    %pushi/vec4 0, 0, 32;
    %load/vec4 v0x18c7180_0;
    %subi 2147418112, 0, 32;
    %ix/vec4/s 3;
    %ix/load 4, 0, 0; Constant delay
    %assign/vec4/a/d v0x18c70c0, 0, 4;
    %load/vec4 v0x18c7180_0;
    %addi 1, 0, 32;
    %store/vec4 v0x18c7180_0, 0, 32;
    %jmp T_0.0;
T_0.1 ;
    %end;
    .thread T_0;
    .scope S_0x18ab670;
T_1 ;
    %wait E_0x18ac100;
    %load/vec4 v0x18c7270_0;
    %pad/u 32;
    %cmpi/e 1, 0, 32;
    %jmp/0xz  T_1.0, 4;
    %vpi_call 3 37 "$display", " in mem_write" {0 0 0};
    %load/vec4 v0x18c7460_0;
    %load/vec4 v0x18ab870_0;
    %subi 2147418112, 0, 32;
    %ix/vec4 3;
    %ix/load 4, 0, 0; Constant delay
    %assign/vec4/a/d v0x18c70c0, 0, 4;
T_1.0 ;
    %load/vec4 v0x18c7270_0;
    %pad/u 32;
    %cmpi/e 0, 0, 32;
    %jmp/0xz  T_1.2, 4;
    %vpi_call 3 42 "$display", " in mem read with address %x", v0x18ab870_0 {0 0 0};
    %load/vec4 v0x18ab870_0;
    %cmpi/u 1879048192, 0, 32;
    %jmp/0xz  T_1.4, 5;
    %pushi/vec4 0, 0, 32;
    %assign/vec4 v0x18c7330_0, 0;
    %jmp T_1.5;
T_1.4 ;
    %load/vec4 v0x18ab870_0;
    %subi 2147418112, 0, 32;
    %ix/vec4 4;
    %load/vec4a v0x18c70c0, 4;
    %assign/vec4 v0x18c7330_0, 0;
T_1.5 ;
T_1.2 ;
    %jmp T_1;
    .thread T_1, $push;
    .scope S_0x18afdb0;
T_2 ;
    %delay 100, 0;
    %load/vec4 v0x18c7740_0;
    %addi 1, 0, 2;
    %store/vec4 v0x18c7740_0, 0, 2;
    %jmp T_2;
    .thread T_2;
    .scope S_0x18afdb0;
T_3 ;
    %pushi/vec4 1, 0, 32;
    %store/vec4 v0x18c79f0_0, 0, 32;
    %pushi/vec4 2147418112, 0, 32;
    %store/vec4 v0x18c75c0_0, 0, 32;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x18c76a0_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x18c7830_0, 0, 1;
    %pushi/vec4 0, 0, 2;
    %store/vec4 v0x18c7740_0, 0, 2;
    %vpi_call 2 36 "$dumpfile", "data_memory_2.vcd" {0 0 0};
    %vpi_call 2 37 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x18afdb0 {0 0 0};
    %end;
    .thread T_3;
    .scope S_0x18afdb0;
T_4 ;
    %wait E_0x18abb70;
    %load/vec4 v0x18c7740_0;
    %cmpi/e 0, 0, 2;
    %jmp/0xz  T_4.0, 4;
    %vpi_call 2 46 "$display", "reading from just initialized data memory: %x", v0x18c7900_0 {0 0 0};
T_4.0 ;
    %load/vec4 v0x18c7740_0;
    %cmpi/e 1, 0, 2;
    %jmp/0xz  T_4.2, 4;
    %vpi_call 2 50 "$display", "writing %x, read_data val: %x", v0x18c79f0_0, v0x18c7900_0 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x18c7830_0, 0;
T_4.2 ;
    %load/vec4 v0x18c7740_0;
    %cmpi/e 2, 0, 2;
    %jmp/0xz  T_4.4, 4;
    %vpi_call 2 55 "$display", "reading %x", v0x18c7900_0 {0 0 0};
    %vpi_call 2 56 "$finish" {0 0 0};
T_4.4 ;
    %jmp T_4;
    .thread T_4, $push;
