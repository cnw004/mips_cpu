    %store/vec4 v0x282a990_0, 0, 32;
    %jmp T_0.3;
T_0.3 ;
    %pop/vec4 1;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x28156b0;
T_1 ;
    %pushi/vec4 344865, 0, 32;
