    %store/vec4 v0x282a990_0, 0, 32;
    %jmp T_0.3;
T_0.1 ;
    %load/vec4 v0x282a7c0_0;
    %store/vec4 v0x282a990_0, 0, 32;
    %jmp T_0.3;
T_0.2 ;
