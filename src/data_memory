#! /usr/remote/bin/vvp
:ivl_version "10.1 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x12026a0 .scope module, "testbench" "testbench" 2 34;
 .timescale 0 0;
P_0x1202820 .param/l "ADDRESS_LOWER_LIMIT" 0 2 45, C4<01111111111111110000000000000000>;
P_0x1202860 .param/l "ADDRESS_UPPER_LIMIT" 0 2 44, C4<01111111111111111111111111111100>;
P_0x12028a0 .param/l "END_COUNT" 0 2 42, +C4<00000000000000000001000000000011>;
v0x122f6d0_0 .var "address", 31 0;
v0x122f7b0_0 .var "clk", 0 0;
v0x122f850_0 .var "count", 4099 0;
v0x122f940_0 .var "mem_write", 0 0;
v0x122fa10_0 .net "read_data", 31 0, v0x122f440_0;  1 drivers
v0x122fb00_0 .var "write_data", 31 0;
E_0x12052f0 .event posedge, v0x122f7b0_0;
S_0x12048e0 .scope module, "data_mem" "data_memory" 2 47, 2 13 0, S_0x12026a0;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "mem_write"
    .port_info 1 /INPUT 32 "address"
    .port_info 2 /INPUT 32 "write_data"
    .port_info 3 /OUTPUT 32 "read_data"
v0x1204b50_0 .net "address", 31 0, v0x122f6d0_0;  1 drivers
v0x122f2b0 .array "data_mem", 2147483644 2147418112, 31 0;
v0x122f370_0 .net "mem_write", 0 0, v0x122f940_0;  1 drivers
v0x122f440_0 .var "read_data", 31 0;
v0x122f520_0 .net "write_data", 31 0, v0x122fb00_0;  1 drivers
E_0x1203000 .event edge, v0x1204b50_0, v0x122f370_0;
    .scope S_0x12048e0;
T_0 ;
    %wait E_0x1203000;
    %load/vec4 v0x122f370_0;
    %pad/u 32;
    %cmpi/e 1, 0, 32;
    %jmp/0xz  T_0.0, 4;
    %load/vec4 v0x122f520_0;
    %load/vec4 v0x1204b50_0;
    %subi 2147418112, 0, 32;
    %ix/vec4 4;
    %store/vec4a v0x122f2b0, 4, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x1204b50_0;
    %subi 2147418112, 0, 32;
    %ix/vec4 4;
    %load/vec4a v0x122f2b0, 4;
    %store/vec4 v0x122f440_0, 0, 32;
T_0.1 ;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x12026a0;
T_1 ;
    %delay 10, 0;
    %load/vec4 v0x122f7b0_0;
    %inv;
    %store/vec4 v0x122f7b0_0, 0, 1;
    %jmp T_1;
    .thread T_1;
    .scope S_0x12026a0;
T_2 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x122f7b0_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x122f940_0, 0, 1;
    %pushi/vec4 2147418112, 0, 32;
    %store/vec4 v0x122f6d0_0, 0, 32;
    %pushi/vec4 0, 0, 4100;
    %store/vec4 v0x122f850_0, 0, 4100;
    %vpi_call 2 62 "$monitor", "count is %4d, address is %x", v0x122f850_0, v0x122f6d0_0 {0 0 0};
    %end;
    .thread T_2;
    .scope S_0x12026a0;
T_3 ;
    %wait E_0x12052f0;
    %load/vec4 v0x122f850_0;
    %addi 1, 0, 4100;
    %store/vec4 v0x122f850_0, 0, 4100;
    %load/vec4 v0x122f6d0_0;
    %addi 16, 0, 32;
    %store/vec4 v0x122f6d0_0, 0, 32;
    %pushi/vec4 4099, 0, 4100;
    %load/vec4 v0x122f850_0;
    %cmp/u;
    %jmp/0xz  T_3.0, 5;
    %vpi_call 2 76 "$display", "count has exceeded its upper bound... ending program\012" {0 0 0};
    %pushi/vec4 0, 0, 4100;
    %store/vec4 v0x122f850_0, 0, 4100;
    %pushi/vec4 2147418112, 0, 32;
    %store/vec4 v0x122f6d0_0, 0, 32;
    %vpi_call 2 79 "$finish" {0 0 0};
T_3.0 ;
    %pushi/vec4 2147483644, 0, 32;
    %load/vec4 v0x122f6d0_0;
    %cmp/u;
    %flag_mov 8, 5;
    %load/vec4 v0x122f6d0_0;
    %cmpi/u 2147418112, 0, 32;
    %flag_or 5, 8;
    %jmp/0xz  T_3.2, 5;
    %vpi_call 2 83 "$display", "resetting values\012address is %x, has gone out of bounds... ending program\012", v0x122f6d0_0 {0 0 0};
    %pushi/vec4 2147418112, 0, 32;
    %store/vec4 v0x122f6d0_0, 0, 32;
    %pushi/vec4 0, 0, 4100;
    %store/vec4 v0x122f850_0, 0, 4100;
    %vpi_call 2 86 "$finish" {0 0 0};
T_3.2 ;
    %jmp T_3;
    .thread T_3;
# The file index is used to find the file name in the following table.
:file_names 3;
    "N/A";
    "<interactive>";
    "data_memory.v";
